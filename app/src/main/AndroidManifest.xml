<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.huandog_v02">

    <application
        android:name="com.example.huandog_v02.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HuanDog_V02">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>


        <activity android:name=".Notice"/>
        <activity android:name=".Loginpage"/>
        <activity android:name=".Joinpage"/>

        <activity android:name=".InfoPage"/>
        <activity android:name=".StartActivity"/>
        <activity android:name=".StartCount"/>



    </application>
    
    <!--// 안드로이드가 화면이 어두워져 있을 때 (잠금상태일 때)화면을 깨울 때 주는 권한.-->
    <uses-permission android:name="androi.permission.WAKE_LOCK"/>

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <uses-feature android:name="android.hardware.camera2"/>
    

</manifest>